{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "barcode": {
      "type": "biginteger",
      "unique": true
    },
    "productName": {
      "type": "string",
      "unique": true
    },
    "ingredients": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::ingredient.ingredient",
      "inversedBy": "products"
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "food",
        "medicine",
        "cosmetica"
      ]
    },
    "land": {
      "type": "string"
    },
    "explanation": {
      "type": "text"
    },
    "store": {
      "type": "string"
    },
    "allIngredients": {
      "type": "text"
    },
    "verificationComments": {
      "type": "richtext"
    },
    "verified": {
      "type": "boolean"
    },
    "lastVerified": {
      "type": "datetime"
    },
    "category": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::drink.drink"
    },
    "scannedAmount": {
      "type": "integer"
    }
  }
}
